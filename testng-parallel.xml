<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<!-- we can specify what we want to run parallel - tests, methods -->
<!-- thread-count is used by testng to generate threads -->
<suite  name="Parallel_Testsuite" parallel="tests" thread-count="2">
  <test name="Test 1">
    <classes>
      <class name="testNgClasses.TestNg_Parallel1"/>
    </classes>
  </test> <!-- Test -->
  <test name="Test 2">
    <classes>
      <class name="testNgClasses.TestNg_Parallel2"/>
    </classes>
  </test>
</suite> <!-- Suite -->
